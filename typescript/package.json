{
  "name": "router",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --ssl true --ssl-cert ./ssl/server.crt --ssl-key ./ssl/server.key",
    "build": "mkdir ./node_modules/@angular/core;cp -rf ./node_modules/@angular/compiler/package.json ./node_modules/@angular/core;ng build",
    "test": "ng test",
    "jest": "jest",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "serve": "mkdir ./node_modules/@angular/core;cp -rf ./node_modules/@angular/compiler/package.json ./node_modules/@angular/core;ng serve router",
    "storybook": "start-storybook",
    "vue-serve": "vue-cli-service serve",
    "vue-build": "vue-cli-service build",
    "vue-lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "dev-vue": "cd vue/vue3/vue-next;node scripts/dev.js",
    "build-vue": "node scripts/build.js",
    "size-runtime": "node scripts/build.js runtime-dom size-check -p -f global",
    "size-compiler": "node scripts/build.js compiler-dom -p -f global",
    "size": "yarn size-runtime && yarn size-compiler",
    "lint-vue": "prettier --write --parser typescript \"vue/vue3/**/*.ts?(x)\"",
    "test-vue": "node scripts/build.js vue -f global -d && jest",
    "test-dts": "node scripts/build.js reactivity runtime-core runtime-dom -t -f esm && tsd",
    "release": "node scripts/release.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "private": true,
